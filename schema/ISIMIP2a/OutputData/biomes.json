{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2a/OutputData/biomes.json",
  "type": "object",
  "properties": {
    "identifiers": {
      "required": [
        "modelname",
        "climate_forcing",
        "bias_adjustment",
        "climate_scenario",
        "soc_scenario",
        "sens_scenario",
        "variable",
        "region",
        "timestep"
      ],
      "properties": {
        "modelname": {
          "type": "string"
        },
        "climate_forcing": {
          "type": "string",
          "enum": [
            "gswp3",
            "princeton",
            "watch",
            "wfdei"
          ]
        },
        "bias_adjustment": {
          "type": "string",
          "enum": [
            "nobc"
          ]
        },
        "climate_scenario": {
          "type": "string",
          "enum": [
            "hist"
          ]
        },
        "soc_scenario": {
          "type": "string",
          "enum": [
            "nosoc",
            "pressoc",
            "varsoc",
            "nat"
          ]
        },
        "sens_scenario": {
          "type": "string",
          "enum": [
            "co2"
          ]
        },
        "variable": {
          "type": "string",
          "enum": [
            "qtot",
            "qs",
            "evap",
            "soilmoist",
            "soilmoistfroz",
            "tsl",
            "snd",
            "swe",
            "thawdepth",
            "cveg",
            "cvegag",
            "cvegbg",
            "clitter",
            "csoil",
            "gpp",
            "ra",
            "npp",
            "npplandleaf",
            "npplandroot",
            "nppabovegroundwood",
            "nppbelowgroundwood",
            "rh",
            "fireint",
            "firefrac",
            "ecoatmflux",
            "rr",
            "fapar",
            "lai",
            "pft",
            "intercep",
            "esoil",
            "trans",
            "cleaf",
            "cwood",
            "croot",
            "burntarea",
            "pft2storeys"
          ]
        },
        "pft": {
          "type": "string"
        },
        "region": {
          "type": "string",
          "enum": [
            "global"
          ]
        },
        "timestep": {
          "type": "string",
          "enum": [
            "3hr",
            "daily",
            "monthly",
            "annual"
          ]
        },
        "start_year": {
          "type": "number",
          "minimum": 1600,
          "maximum": 2100
        },
        "end_year": {
          "type": "number",
          "minimum": 1600,
          "maximum": 2100
        }
      }
    }
  }
}