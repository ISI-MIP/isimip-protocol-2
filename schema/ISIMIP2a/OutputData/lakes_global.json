{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2a/OutputData/lakes_global.json",
  "type": "object",
  "required": [
    "model",
    "climate_forcing",
    "bias_adjustment",
    "climate_scenario",
    "soc_scenario",
    "sens_scenario",
    "variable",
    "region",
    "timestep"
  ],
  "properties": {
    "model": {
      "type": "string"
    },
    "climate_forcing": {
      "type": "string",
      "enum": [
        "ewembi"
      ]
    },
    "bias_adjustment": {
      "type": "string",
      "enum": [
        "nobc"
      ]
    },
    "climate_scenario": {
      "type": "string",
      "enum": [
        "hist"
      ]
    },
    "soc_scenario": {
      "type": "string",
      "enum": [
        "nosoc",
        "pressoc",
        "varsoc",
        "nat"
      ]
    },
    "sens_scenario": {
      "type": "string",
      "enum": [
        "co2"
      ]
    },
    "variable": {
      "type": "string",
      "enum": [
        "strat",
        "thermodepth",
        "watertemp",
        "surftemp",
        "bottemp",
        "ice",
        "lakeicefrac",
        "icethick",
        "snowthick",
        "icetemp",
        "snowtemp",
        "sensheatf",
        "latentheatf",
        "momf",
        "swup",
        "lwup",
        "lakeheatf",
        "turbdiffheat",
        "albedo",
        "extcoeff",
        "sedheatf",
        "chl",
        "phytobio",
        "zoobio",
        "tp",
        "pp",
        "tpd",
        "tn",
        "pn",
        "tdn",
        "do",
        "doc",
        "si",
        "swdw"
      ]
    },
    "region": {
      "type": "string",
      "enum": [
        "global"
      ]
    },
    "timestep": {
      "type": "string",
      "enum": [
        "3hr",
        "daily",
        "monthly",
        "annual"
      ]
    },
    "start_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    },
    "end_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    }
  }
}