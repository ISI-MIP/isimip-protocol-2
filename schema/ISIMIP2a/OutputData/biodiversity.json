{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2a/OutputData/biodiversity.json",
  "type": "object",
  "required": [
    "modelname",
    "climate_forcing",
    "bias_adjustment",
    "climate_scenario",
    "soc_scenario",
    "sens_scenario",
    "variable",
    "timestep"
  ],
  "properties": {
    "modelname": {
      "type": "string"
    },
    "climate_forcing": {
      "type": "string",
      "enum": [
        "ewembi"
      ]
    },
    "bias_adjustment": {
      "type": "string",
      "enum": [
        "nobc"
      ]
    },
    "climate_scenario": {
      "type": "string",
      "enum": [
        "hist"
      ]
    },
    "soc_scenario": {
      "type": "string",
      "enum": [
        "nosoc"
      ]
    },
    "sens_scenario": {
      "type": "string",
      "enum": [
        "co2"
      ]
    },
    "variable": {
      "type": "string",
      "enum": [
        "amphibianprob",
        "birdprob",
        "mammalprob",
        "amphibiansumprob",
        "birdsumprob",
        "mammalsumprob",
        "endamphibiansumprob",
        "endbirdsumprob",
        "endmammalsumprob",
        "thramphibiansumprob",
        "thrbirdsumprob",
        "thrmammalsumprob",
        "amphibiansr",
        "birdsr",
        "mammalsr"
      ]
    },
    "region": {
      "type": "string",
      "enum": [
        "global"
      ]
    },
    "timestep": {
      "type": "string",
      "enum": [
        "30year-mean"
      ]
    },
    "start_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    },
    "end_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    }
  }
}