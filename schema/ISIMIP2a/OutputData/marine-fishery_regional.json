{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2a/OutputData/marine-fishery_regional.json",
  "type": "object",
  "required": [
    "model",
    "climate_forcing",
    "bias_adjustment",
    "climate_scenario",
    "variable",
    "ocean_region",
    "timestep"
  ],
  "properties": {
    "model": {
      "type": "string"
    },
    "climate_forcing": {
      "type": "string",
      "enum": [
        "gfdl-reanalysis",
        "gfdl-esm2m",
        "ipsl-cm5a-lr",
        "key-run",
        "cesm1-bgc"
      ]
    },
    "bias_adjustment": {
      "type": "string",
      "enum": [
        "nobc"
      ]
    },
    "climate_scenario": {
      "type": "string",
      "enum": [
        "hist",
        "rcp8p5",
        "rcp2p6",
        "rcp4p5",
        "rcp6p0",
        "fore"
      ]
    },
    "variable": {
      "type": "string",
      "enum": [
        "tsb",
        "tcb",
        "b10cm",
        "b30cm",
        "tc",
        "tla",
        "bcom",
        "blarge",
        "bmed",
        "bsmall",
        "b",
        "clarge",
        "cmed",
        "csmall",
        "c",
        "tc10cm",
        "tc30cm"
      ]
    },
    "ocean_region": {
      "type": "string",
      "enum": [
        "north-sea",
        "baltic-sea",
        "nw-med-sea",
        "adriatic-sea",
        "med-glob",
        "se-australia",
        "east-bass-strait",
        "cook-strait",
        "humboldt-n"
      ]
    },
    "timestep": {
      "type": "string",
      "enum": [
        "3hr",
        "daily",
        "monthly",
        "annual"
      ]
    },
    "start_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    },
    "end_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    }
  }
}