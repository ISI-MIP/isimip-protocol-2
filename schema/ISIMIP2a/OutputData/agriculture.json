{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2a/OutputData/agriculture.json",
  "type": "object",
  "properties": {
    "dimensions": {
      "type": "object",
      "required": [
        "lat",
        "lon",
        "time"
      ],
      "properties": {
        "lat": {
          "const": 360
        },
        "lon": {
          "const": 720
        }
      }
    },
    "variables": {
      "type": "object",
      "required": [
        "lat",
        "lon",
        "time"
      ],
      "properties": {
        "lat": {
          "type": "object",
          "required": [
            "axis",
            "long_name",
            "units"
          ],
          "properties": {
            "axis": {
              "const": "Y"
            },
            "units": {
              "const": "degrees_north"
            }
          }
        },
        "lon": {
          "type": "object",
          "required": [
            "axis",
            "long_name",
            "units"
          ],
          "properties": {
            "axis": {
              "const": "X"
            },
            "units": {
              "const": "degrees_east"
            }
          }
        },
        "time": {
          "type": "object",
          "required": [
            "long_name",
            "units"
          ],
          "properties": {
            "axis": {
              "const": "T"
            },
            "units": {
              "enum": [
                "days since 1901-01-01",
                "days since 1901-01-01 00:00:00",
                "days since 1901-1-1",
                "days since 1901-1-1 00:00:00",
                "months since 1901-01-01",
                "months since 1901-01-01 00:00:00",
                "months since 1901-01-01",
                "months since 1901-01-01 00:00:00",
                "years since 1901-01-01",
                "years since 1901-01-01 00:00:00",
                "years since 1901-01-01",
                "years since 1901-01-01 00:00:00"
              ]
            }
          }
        }
      }
    },
    "specifiers": {
      "type": "object",
      "required": [
        "model",
        "climate_forcing",
        "bias_adjustment",
        "climate_scenario",
        "harmonization",
        "variable",
        "crop",
        "irrigation",
        "region",
        "timestep"
      ],
      "properties": {
        "model": {
          "type": "string"
        },
        "climate_forcing": {
          "type": "string",
          "enum": [
            "gswp3",
            "princeton",
            "watch",
            "wfdei"
          ]
        },
        "bias_adjustment": {
          "type": "string",
          "enum": [
            "nobc"
          ]
        },
        "climate_scenario": {
          "type": "string",
          "enum": [
            "hist"
          ]
        },
        "harmonization": {
          "type": "string"
        },
        "variable": {
          "type": "string",
          "enum": [
            "pirrww",
            "yield",
            "aet",
            "initr",
            "plantday",
            "anthday",
            "matyday",
            "harvday",
            "biom",
            "sco2",
            "sn2o",
            "tnup",
            "tnin",
            "tnloss",
            "leach",
            "gsprcp",
            "gsrsds",
            "sumt"
          ]
        },
        "crop": {
          "type": "string",
          "enum": [
            "whe",
            "mai",
            "soy",
            "ric",
            "bar",
            "ben",
            "cas",
            "cot",
            "euc",
            "mgr",
            "mil",
            "mis",
            "nut",
            "pea",
            "pop",
            "pot",
            "rap",
            "ri2",
            "rye",
            "sgb",
            "sor",
            "sug",
            "sun"
          ]
        },
        "irrigation": {
          "type": "string",
          "enum": [
            "firr",
            "noirr"
          ]
        },
        "region": {
          "type": "string",
          "enum": [
            "global"
          ]
        },
        "timestep": {
          "type": "string",
          "enum": [
            "annual"
          ]
        },
        "start_year": {
          "type": "number",
          "minimum": 1600,
          "maximum": 2100
        },
        "end_year": {
          "type": "number",
          "minimum": 1600,
          "maximum": 2100
        }
      }
    }
  }
}