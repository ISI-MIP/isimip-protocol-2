{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2a/OutputData/agriculture.json",
  "type": "object",
  "required": [
    "modelname",
    "climate_forcing",
    "bias_adjustment",
    "climate_scenario",
    "harmonization",
    "variable",
    "crop",
    "irrigation",
    "region",
    "timestep"
  ],
  "properties": {
    "modelname": {
      "type": "string"
    },
    "climate_forcing": {
      "type": "string",
      "enum": [
        "gswp3",
        "princeton",
        "watch",
        "wfdei"
      ]
    },
    "bias_adjustment": {
      "type": "string",
      "enum": [
        "nobc"
      ]
    },
    "climate_scenario": {
      "type": "string",
      "enum": [
        "hist"
      ]
    },
    "harmonization": {
      "type": "string"
    },
    "variable": {
      "type": "string",
      "enum": [
        "pirrww",
        "yield",
        "aet",
        "initr",
        "plantday",
        "anthday",
        "matyday",
        "harvday",
        "biom",
        "sco2",
        "sn2o",
        "tnup",
        "tnin",
        "tnloss",
        "leach",
        "gsprcp",
        "gsrsds",
        "sumt"
      ]
    },
    "crop": {
      "type": "string",
      "enum": [
        "whe",
        "mai",
        "soy",
        "ric",
        "bar",
        "ben",
        "cas",
        "cot",
        "euc",
        "mgr",
        "mil",
        "mis",
        "nut",
        "pea",
        "pop",
        "pot",
        "rap",
        "ri2",
        "rye",
        "sgb",
        "sor",
        "sug",
        "sun"
      ]
    },
    "irrigation": {
      "type": "string",
      "enum": [
        "firr",
        "noirr"
      ]
    },
    "region": {
      "type": "string",
      "enum": [
        "global"
      ]
    },
    "timestep": {
      "type": "string",
      "enum": [
        "annual"
      ]
    },
    "start_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    },
    "end_year": {
      "type": "number",
      "minimum": 1600,
      "maximum": 2100
    }
  }
}