{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2b/OutputData/biomes.json",
  "type": "object",
  "required": [
    "modelname",
    "climate_forcing",
    "bias_adjustment",
    "climate_scenario",
    "soc_scenario",
    "sens_scenario",
    "variable",
    "region",
    "timestep"
  ],
  "properties": {
    "modelname": {
      "type": "string"
    },
    "climate_forcing": {
      "type": "string",
      "enum": [
        "gfdl-esm2m",
        "hadgem2-es",
        "ipsl-cm5a-lr",
        "miroc5"
      ]
    },
    "bias_adjustment": {
      "type": "string",
      "enum": [
        "nobc",
        "ewembi"
      ]
    },
    "climate_scenario": {
      "type": "string",
      "enum": [
        "picontrol",
        "historical",
        "rcp26",
        "rcp60",
        "rcp85"
      ]
    },
    "sens_scenario": {
      "type": "string"
    },
    "variable": {
      "type": "string",
      "enum": [
        "qtot",
        "qs",
        "evap",
        "soilmoist",
        "soilmoistfroz",
        "tsl",
        "snd",
        "swe",
        "thawdepth",
        "cveg",
        "cvegag",
        "cvegbg",
        "clitter",
        "csoil",
        "gpp",
        "ra",
        "npp",
        "npplandleaf",
        "npplandroot",
        "nppabovegroundwood",
        "nppbelowgroundwood",
        "rh",
        "fireint",
        "firefrac",
        "ecoatmflux",
        "rr",
        "fapar",
        "lai",
        "pft",
        "intercep",
        "esoil",
        "trans",
        "cleaf",
        "cwood",
        "croot",
        "burntarea",
        "pft2storeys",
        "albedo",
        "somcfire",
        "rainf"
      ]
    },
    "pft": {
      "type": "string"
    },
    "region": {
      "type": "string",
      "enum": [
        "global"
      ]
    },
    "timestep": {
      "type": "string",
      "enum": [
        "3hr",
        "daily",
        "monthly",
        "annual",
        "constant"
      ]
    },
    "start_year": {
      "type": "number",
      "minimum": 1661,
      "maximum": 2300
    },
    "end_year": {
      "type": "number",
      "minimum": 1661,
      "maximum": 2300
    }
  }
}