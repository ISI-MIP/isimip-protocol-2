{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://isi-mip.github.io/isimip-protocol-2/schema/ISIMIP2b/OutputData/marine-fishery_global.json",
  "type": "object",
  "required": [
    "modelname",
    "climate_forcing",
    "bias_adjustment",
    "climate_scenario",
    "variable",
    "region",
    "timestep"
  ],
  "properties": {
    "modelname": {
      "type": "string"
    },
    "climate_forcing": {
      "type": "string",
      "enum": [
        "gfdl-esm2m",
        "hadgem2-es",
        "ipsl-cm5a-lr",
        "miroc5"
      ]
    },
    "bias_adjustment": {
      "type": "string",
      "enum": [
        "nobc",
        "ewembi"
      ]
    },
    "climate_scenario": {
      "type": "string",
      "enum": [
        "hist",
        "rcp2p6",
        "rcp4p5",
        "rcp6p0",
        "rcp8p5"
      ]
    },
    "sens_scenario": {
      "type": "string"
    },
    "variable": {
      "type": "string",
      "enum": [
        "tsb",
        "tcb",
        "b10cm",
        "b30cm",
        "tc",
        "tla",
        "bcom",
        "blarge",
        "bmed",
        "bsmall",
        "b",
        "clarge",
        "cmed",
        "csmall",
        "c",
        "tc10cm",
        "tc30cm",
        "b10cm-bendet",
        "b30cm-bendet"
      ]
    },
    "region": {
      "type": "string",
      "enum": [
        "global"
      ]
    },
    "timestep": {
      "type": "string",
      "enum": [
        "3hr",
        "daily",
        "monthly",
        "annual"
      ]
    },
    "start_year": {
      "type": "number",
      "minimum": 1661,
      "maximum": 2300
    },
    "end_year": {
      "type": "number",
      "minimum": 1661,
      "maximum": 2300
    }
  }
}